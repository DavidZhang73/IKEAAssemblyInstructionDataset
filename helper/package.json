{
  "name": "ikea-assembly-instruction-dataset-helper",
  "version": "0.1.0",
  "author": "Jiahao Zhang",
  "description": "A tool for Ikea Assembly Instruction Dataset.",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:vite": "vite",
    "dev:electron": "electron src/main/index.js",
    "build": "run-s build:*",
    "build:vite": "vite build --base=./",
    "build:electron": "electron-builder build",
    "postinstall": "install-app-deps"
  },
  "dependencies": {
    "@headlessui/vue": "^1.4.2",
    "@heroicons/vue": "^1.0.5",
    "electron-context-menu": "^3.0.0",
    "electron-ipc-proxy": "^2.1.0",
    "electron-window-state": "^5.0.3",
    "lodash-es": "^4.17.21",
    "mongodb": "^4.2.1",
    "vue": "^3.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/compiler-sfc": "^3.0.11",
    "autoprefixer": "^10.2.5",
    "electron": "^16.0.0",
    "electron-builder": "^22.10.5",
    "electron-reloader": "^1.2.1",
    "vite": "^2.2.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.13",
    "tailwindcss": "^3.0.15",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0"
  },
  "peerDependencies": {
    "postcss": "^8.2.13"
  },
  "main": "src/main/index.js",
  "resolutions": {
    "postcss": "^8.2.13"
  },
  "build": {
    "directories": {
      "output": "build"
    },
    "mac": {
      "target": "dmg"
    },
    "files": [
      "src/main/index.js",
      "src/main",
      "dist",
      "package.json"
    ]
  }
}
